    "version": "0.2.0",
    "configurations": [
        {
            "name": "Circle RANSAC",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/devel/lib/circle_ransac/circle_ransac_node",
            "args": ["./././src/circle_ransac/src/points1.txt"],
            // "args": ["./././src/circle_ransac/src/points2.txt"],
            // "args": ["./././src/circle_ransac/src/points3.txt"],
            // "args": ["./././src/circle_ransac/src/points4.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Sphere RANSAC",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/devel/lib/sphere_ransac/sphere_ransac_node",
            "args": ["./././src/sphere_ransac/src/Sphere1.xyz"],
            // "args": ["./././src/sphere_ransac/src/Sphere2.xyz"],
            // "args": ["./././src/sphere_ransac/src/Sphere3.xyz"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Homography",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/devel/lib/homography/homography_node",
            // "args": ["./././src/homography/src/11.jpg", "./././src/homography/src/1.jpg"],
            // "args": ["./././src/homography/src/22.jpg", "./././src/homography/src/2.jpg"],
            "args": ["./././src/homography/src/33.jpg", "./././src/homography/src/3.jpg"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}